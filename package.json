{
  "name": "revolt-radio",
  "version": "1.0.0",
  "description": "REVOLT Radio",
  "main": "index.ts",
  "scripts": {
    "electron-dev": "concurrently \"npm start\" \"npm run tsc:dev",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "tsc:dev": "tsc-watch --onSuccess \"electron ./dist/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/andrewburgess/revolt-radio.git"
  },
  "author": "Andrew Burgess <andrew@andrewburgess.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewburgess/revolt-radio/issues"
  },
  "homepage": "https://github.com/andrewburgess/revolt-radio#readme",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@svgr/webpack": "^4.3.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
